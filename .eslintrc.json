{
    "extends": [
        "plugin:import/errors",
        "next/core-web-vitals"
    ],
    "settings": {
        "import/resolver": {
            "alias": {
                "extensions": [".js", ".jsx"],
                "map": [["@", "./src"]]
            }
        }
    },
    "rules": {
        "react/prop-types": "off",
        "no-unused-vars": "warn",
    }
}
